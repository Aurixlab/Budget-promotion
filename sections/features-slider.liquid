{% comment %}
  Features with Image Slider Section
  Add this to your sections folder as 'features-slider.liquid'
{% endcomment %}

<style>
        .features-slider-wrapper {
          display: grid;
          grid-template-columns: 1fr 1fr;
          gap: 2rem;
          padding: 2rem 1rem;
          max-width: 100%;
          margin: 0 auto;
        }
      .slide__overlay{
          position: absolute;
          left: 0;
          top: 0;
          width: 100%;
          height: 100%;
          background: linear-gradient(0deg, #000000a8, transparent);
      }
        /* Left Side - Static Features */
        .features-box {
          background: {{ section.settings.features_bg_color }};
          border: 3px solid {{ section.settings.features_border_color }};
          display: flex;
          flex-direction: column;
        }

        .features-box__title {
          font-size: clamp(2rem, 4vw, 4rem);
          font-weight: 700;
          color: {{ section.settings.features_title_color }};
          line-height: 1.1;
          margin-bottom: 2rem;
          text-transform: uppercase;
              letter-spacing: 0;
              max-width: 680px;
    width: 100%;
        }

        .features-box__cta {
          margin-bottom: 2rem;
          width: fit-content;
          transition: transform 0.3s ease;
          display: inline-block;
          padding: .7rem 2.5rem;
          background-color: #ff741c;
          color: #ffffff !important;
          border-radius: 38px;
          font-weight: 600;
          text-transform: uppercase;
        }

        .features-box__cta:hover {
          transform: scale(1.05);
        }

        .features-grid {
          display: grid;
          grid-template-columns: repeat(4, 1fr);
          gap: 2rem 1.5rem;
          margin-top: 2rem;
        }

        .feature-col {
  position: relative;
      height: 140px;
      background: #F2F2F2;
      box-shadow: 0px 4px 4px rgba(0, 0, 0, 0.25);
      border-radius: 25px;
      display: flex;
      align-content: center;
      justify-content: center;
      align-items: center;
  }

        .feature-item {
          display: flex;
          flex-direction: column;
          align-items: center;
          text-align: center;
            position: absolute;
    top: 0;
    left: 0;
    opacity: 0;
    transition: opacity .6s ease;
    width:100%;
    height:100%;
    padding:20px 10px;
        }

        .feature-item.active {
    opacity: 1;
  }

        .feature-item__icon {
          width: 60px;
          height: 60px;
          margin-bottom: .5rem;
          display: flex;
          align-items: center;
          justify-content: center;
        }

        .feature-item__icon svg {
          width: 100%;
          height: 100%;
        }

        .feature-item__text {
      font-style: normal;
      font-weight: 700;
      font-size: 16px;
      line-height: 19px;
      text-align: center;
      text-transform: uppercase;
      color: #000000;
        }

        .services-grid {
          display: grid;
          grid-template-columns: repeat(2, 1fr);
          gap: 1rem;
          margin-top: auto;
              align-items: start;
        }

      .service-card {
        position: relative;
        border-radius: 25px;
        overflow: hidden;
        cursor: pointer;
        transition: all 0.4s ease;
        height: 200px;
        background: white;
        box-shadow: 0 4px 15px rgba(0, 0, 0, 0.1);
        display: flex;
        flex-direction: column;
      }

      .service-card:hover {
        transform: translateY(-8px);
        box-shadow: 0 12px 30px rgba(255, 107, 53, 0.25);
      }

      .service-image-container {
        height: 150px;
        position: relative;
        overflow: hidden;
      }

      .service-image {
        width: 100%;
        height: 100%;
        object-fit: cover;
        transition: transform 0.6s ease;
      }

        .service-card:hover .service-image {
        transform: scale(1.15);
      }

      .service-overlay {
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        background: linear-gradient(to bottom, rgba(0, 0, 0, 0) 0%, rgba(0, 0, 0, 0.6) 100%);
      }

      .service-content {
        flex: 1;
        padding: 10px 20px;
        display: flex;
        flex-direction: column;
        justify-content: center;
        background: white;
        position: relative;
      }

      .service-title {
        letter-spacing: 0.5px;
        margin-bottom: 0px;
        transition: color 0.3s ease;
        font-style: italic;
        font-weight: 700;
        font-size: 16px;
        line-height: 19px;
        text-transform: uppercase;
        color: #000000;
      }

      .service-card:hover .service-title {
        color: #FF6B35;
      }

      .service-description {
        color: #666;
        font-size: 13px;
        line-height: 1.5;
        opacity: 0;
        max-height: 0;
        overflow: hidden;
        transition: all 0.4s ease;
      }

      .service-card:hover .service-description {
        opacity: 1;
        max-height: 80px;
      }

      .service-icon {
        position: absolute;
        top: 12px;
        right: 12px;
        width: 35px;
        height: 35px;
        background: rgba(255, 107, 53, 0.9);
        border-radius: 50%;
        display: none;
        align-items: center;
        justify-content: center;
        opacity: 0;
        transform: scale(0) rotate(-180deg);
        transition: all 0.4s cubic-bezier(0.68, -0.55, 0.265, 1.55);
        z-index: 10;
      }

      .service-card:hover .service-icon {
        opacity: 1;
        transform: scale(1) rotate(0deg);
      }

      .service-icon::after {
        content: '+';
        color: white;
        font-size: 22px;
        font-weight: 300;
      }

      .service-label {
        position: absolute;
        bottom: 12px;
        left: 12px;
        background: white;
        color: #1a1a1a;
        padding: 6px 14px;
        border-radius: 20px;
        font-size: 11px;
        font-weight: 700;
        text-transform: uppercase;
        letter-spacing: 0.5px;
        box-shadow: 0 2px 8px rgba(0, 0, 0, 0.2);
        z-index: 10;
      }

      @media (max-width: 640px) {
        .services-grid {
          grid-template-columns: repeat(2, 1fr);
          gap: 0.75rem;
        }
    .service-image-container {
        height: 80px;
    }
    .service-card {
        height: auto;
    }

        .service-label {
          font-size: 9px;
          padding: 4px 10px;
          bottom: 8px;
          left: 8px;
        }

        .service-icon {
          width: 25px;
          height: 25px;
          top: 8px;
          right: 8px;
        }

        .service-icon::after {
          font-size: 16px;
        }
      }

        .service-card__label {
      position: absolute;
          top: 50%;
          left: 50%;
          transform: translate(-50%, -50%);
          font-style: normal;
          font-weight: 700;
          font-size: 16px;
          line-height: 19px;
          text-align: center;
          text-transform: uppercase;
          color: #FFFFFF;
          text-shadow: 0px 4px 6.2px #000000;
        }

        /* Right Side - Slider */
        .slider-box {
          background: {{ section.settings.slider_border_color }};
          border: 3px solid {{ section.settings.slider_border_color }};
          padding: 0px;
          position: relative;
          overflow: hidden;
          border-radius: 32px;
        }

        .slider-container {
          position: relative;
          height: 89vh;
          border-radius: 0px;
          overflow: hidden;
        }

        .slide {
          position: absolute;
          top: 0;
          left: 0;
          width: 100%;
          height: 100%;
          opacity: 0;
          transition: opacity 0.6s ease-in-out;
          display: flex;
          flex-direction: column;
          justify-content: flex-end;
          padding: 3rem;
        }

        .slide.active {
          opacity: 1;
          z-index: 1;
        }

        .slide__background {
          position: absolute;
          top: 0;
          left: 0;
          width: 100%;
          height: 100%;
          object-fit: cover;
          z-index: 0;
        }

        {% comment %} .slide__overlay {
          position: absolute;
          top: 0;
          left: 0;
          width: 100%;
          height: 100%;
          background: linear-gradient(to top, rgba(0, 0, 0, 0.7) 0%, transparent 60%);
          z-index: 1;
        } {% endcomment %}

        .slide__content {
          position: relative;
          z-index: 2;
          color: #fff;
          max-width: 600px;
        }

        .slide__button {
          margin-bottom: 1.5rem;
          width: fit-content;
          transition: transform 0.3s ease;
          display: inline-block;
          padding: .7rem 2.5rem;
          background-color: #ff741c;
          color: #ffffff !important;
          border-radius: 38px;
          font-weight: 600;
          text-transform: uppercase;
        }

        .slide__button:hover {
          transform: scale(1.05);
        }

        .slide__title {
          font-size: clamp(2rem, 4vw, 3rem);
          font-weight: 700;
          margin-bottom: 1rem;
          text-transform: uppercase;
          line-height: 58px;
          color: #FFFFFF;
          text-shadow: 0px 7px 0px rgba(0, 0, 0, 0.25);
        }

        .slide__description {
      font-size: 20px;
          line-height: 24px;
          color: #FFFFE4;
        }

        /* Slider Navigation */
        .slider-nav {
          position: absolute;
          top: 2rem;
          right: 2rem;
          z-index: 10;
          display: flex;
          gap: 0.3rem;
        }

        .slider-nav__dot {
          width: 18px;
          height: 18px;
          border-radius: 50%;
          border: 2px solid rgba(255, 255, 255, 0.5);
          background: rgba(255, 255, 255, 0.2);
          cursor: pointer;
          transition: all 0.3s ease;
          display: flex;
          align-items: center;
          justify-content: center;
          overflow: hidden;
        }

        .slider-nav__dot img {
          width: 100%;
          height: 100%;
          object-fit: cover;
          opacity: 0.5;
          transition: opacity 0.3s ease;
        }

        .slider-nav__dot.active {
          border-color: #fff;
          background: rgba(255, 255, 255, 0.3);
        }

        .slider-nav__dot.active img {
          opacity: 1;
        }

        /* Responsive */
        @media (max-width: 1024px) {
          .features-slider-wrapper {
            grid-template-columns: 1fr;
            gap: 1.5rem;
            padding: 1.5rem;
          }

          .features-box,
          .slider-container {
            min-height: 500px !important;
          }

          .features-grid {
            gap: 1.5rem 1rem;
          }

          .services-grid {
            gap: 0.75rem;
          }
        }

      @media (max-width: 400px) {
            .features-grid {
              gap: .5rem;
          }
        }

        @media (max-width: 640px) {
          .features-box,
          .slide__content {
            padding: 0rem;
          }
           .slider-container {
              height: 440px !important;
          }
          .features-slider-wrapper {
                padding-left: 1rem;
              padding-right: 1rem;
              padding-bottom: 2rem;
              gap: 1rem;
          }
          .features-grid {
            grid-template-columns: repeat(2, 2fr);
            gap: .5rem;
            margin-top: 1rem;
          }
          .feature-col {
      height: 110px;
  }
      .feature-item__icon svg {
          width: 70%;
      }
      .feature-item__icon img {
          width: 40px;
      }
      .feature-item__icon {
          margin-bottom: 0;
      }
      .feature-item__text {
          font-size: 11px;
          line-height: 13px;
          margin: 0;
      }

      .features-box__title{
          margin-bottom: 1rem;
      }
      .features-box__cta {
          margin-bottom: 1rem;
          font-size: 14px;
          padding: .5rem 1.5rem;
      }


          .slider-nav {
            top: 1rem;
            right: 1rem;
          }

          .slider-nav__dot {
            width: 14px;
            height: 14px;
          }
          .slide {
          padding: 1.5rem;
      }
      .slide__button {
              padding: .5rem 1.5rem;
              margin-bottom: 0.4rem;
              font-size: 14px;
      }
      .slide__description {
          font-size: 14px;
          line-height: 18px;
      }
      .slide__title {
          margin-bottom: 0rem;
          line-height: 48px;
      }
      }
</style>

<section class="features-slider-section">
  <div class="features-slider-wrapper">
    <!-- Left Side - Static Features -->
    <div class="features-box">
      <h2 class="features-box__title">{{ section.settings.main_title }}</h2>

      <a
        href="{{ section.settings.cta_link }}"
        class="features-box__cta"
      >
        {{ section.settings.cta_text }}
      </a>

      <!-- Service Cards Grid -->
      <div class="services-grid">
        {% for block in section.blocks %}
          {% if block.type == 'service_card' %}
            <div class="service-card">
              <div class="service-image-container">
                {% if block.settings.service_image != blank %}
                  <img
                    src="{{ block.settings.service_image | img_url: '600x400' }}"
                    alt="{{ block.settings.service_name }}"
                    class="service-image"
                    loading="lazy"
                  >
                {% endif %}
                <div class="service-overlay"></div>
                <div class="service-icon"></div>
              </div>
              <div class="service-content">
                <h3 class="service-title">{{ block.settings.service_name }}</h3>
                <p class="service-description">{{ block.settings.service_description }}</p>
              </div>
            </div>
          {% endif %}
        {% endfor %}
      </div>

            <div class="features-grid">
        <div class="feature-col">
          <div class="feature-item">
            <div class="feature-item__icon">
